<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.com.develop.intimex">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".SplashScreen.SplashScreen"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Activitys.MainActivity"
            android:label="@string/app_name" />

        <activity
            android:label="Registrar Producto"
            android:name=".Activitys.RegistrarProducto"
            android:parentActivityName=".Activitys.MainActivity"/>

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".Activitys.MainActivity" />

        <activity
            android:label="Buscador"
            android:name=".Activitys.Buscador"
            android:parentActivityName=".Activitys.MainActivity">
        </activity>

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".Activitys.MainActivity" />

        <activity
            android:label="Contactar"
            android:name=".Activitys.Contactar"
            android:parentActivityName=".Activitys.MainActivity"/>

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".Activitys.MainActivity" />

    </application>

</manifest>